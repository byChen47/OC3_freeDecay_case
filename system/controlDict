FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            (overset fvMotionSolvers sixDoFMooring);

application     overInterDyMFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         300.0;

deltaT          0.01;

writeControl    adjustableRunTime;// adjustableRunTime; timeStep;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable no; //yes;

adjustTimeStep  yes;

maxCo           1; // 5.0
maxAlphaCo      1.5; // 2.0


pressureRefPoints
(
    (-100 -100 70)
);

refPointZoneID
(
    0 
);
